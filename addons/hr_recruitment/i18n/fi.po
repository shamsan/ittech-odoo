"    </t>\n"
"    <br/>\n"
"    <t t-if=\"object.job_id.address_id.country_id.name\">\n"
"        <t t-out=\"object.job_id.address_id.country_id.name or ''\">Argentina</t><br/>\n"
"        <t t-set=\"location\" t-value=\"'%s, %s' % (location, object.job_id.address_id.country_id.name)\"/>\n"
"    </t>\n"
"    <br/>\n"
"</div>\n"
